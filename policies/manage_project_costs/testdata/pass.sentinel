func estimates(input) {
	return {
		"less_than": func(i) {
			return to_float(input) < i
		},
		"less_than_or_equal_to": func(i) {
			return to_float(input) <= i
		},
		"greater_than": func(i) {
			return to_float(input) > i
		},
		"greater_than_or_equal_to": func(i) {
			return to_float(input) >= i
		},
	}
}

func to_float(s) {
	return decimal.new(s).float
}